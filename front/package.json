{
  "name": "front",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "brunch watch --server",
    "build": "rm -rf public && brunch build --production",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "auto-reload-brunch": "latest",
    "brunch": "latest",
    "brunch-typescript": "latest",
    "digest-brunch": "latest",
    "jest": "latest",
    "sanitize.css": "latest",
    "stylus-brunch": "latest",
    "ts-jest": "latest"
  },
  "dependencies": {
    "@types/react": "^0.0.0",
    "@types/react-dom": "^0.14.20",
    "@types/whatwg-fetch": "^0.0.33",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "whatwg-fetch": "^2.0.1"
  },
  "jest": {
    "notify": true,
    "testRegex": "(.test|spec)\\.(ts|tsx|js)$",
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "globals": {
      "__TS_CONFIG__": {
        "module": "commonjs",
        "jsx": "react"
      }
    }
  }
}
